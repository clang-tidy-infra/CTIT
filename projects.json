{
  "projects": {
    "cppcheck": {
      "url": "https://github.com/danmar/cppcheck.git",
      "commit": "1e79f7ffa1de4b9dc34cd96080f7d78c925d19b0",
      "cmake_flags": ["-DBUILD_TESTS=ON", "-DCMAKE_DISABLE_PRECOMPILE_HEADERS=ON"],
      "build_targets": ["cppcheck-core"]
    },
    "llvm-project": {
      "url": "https://github.com/llvm/llvm-project.git",
      "commit": "c04b00de750801238bc85f11874c66a41b61b2c5",
      "cmake_source_subdir": "llvm",
      "cmake_flags": [
        "-DLLVM_ENABLE_PROJECTS=clang;clang-tools-extra",
        "-DCMAKE_DISABLE_PRECOMPILE_HEADERS=ON",
        "-DLLVM_ENABLE_ASSERTIONS=ON",
        "-DLLVM_TARGETS_TO_BUILD=X86",
        "-DLLVM_INCLUDE_BENCHMARKS=OFF",
        "-DLLVM_INCLUDE_EXAMPLES=OFF",
        "-DLLVM_INCLUDE_TESTS=OFF",
        "-DCLANG_TIDY_ENABLE_STATIC_ANALYZER=OFF"
      ],
      "build_targets": [
        "clang",
        "clangAnalysisFlowSensitiveResources",
        "clang-nvlink-wrapper",
        "clang-sycl-linker",
        "clang-installapi",
        "clang-scan-deps",
        "clang-linker-wrapper"
      ],
      "file_regex": "clang/.*(?<!\\.S)$"
    },
    "doxygen": {
      "url": "https://github.com/doxygen/doxygen.git",
      "commit": "29e1b7ca1171a0026c3e928408877d2cd5c7adca",
      "cmake_flags": [
        "-Dbuild_wizard=OFF",
        "-Dbuild_search=OFF",
        "-Dbuild_doc=OFF",
        "-Duse_libclang=OFF",
        "-DCMAKE_DISABLE_PRECOMPILE_HEADERS=ON"
      ],
      "build_targets": ["generate_configvalues_header"]
    },
    "poco": {
      "url": "https://github.com/pocoproject/poco.git",
      "commit": "78a5716858c481631bc66048641c1ba884fc509e",
      "cmake_flags": [
        "-DENABLE_TESTS=OFF",
        "-DENABLE_SAMPLES=OFF",
        "-DCMAKE_DISABLE_PRECOMPILE_HEADERS=ON"
      ]
    },
    "abseil-cpp": {
      "url": "https://github.com/abseil/abseil-cpp.git",
      "commit": "64c1b40d732cccd660df378a1639505bbdbfe5b3",
      "cmake_flags": [
        "-DABSL_BUILD_TESTING=OFF",
        "-DABSL_BUILD_TEST_HELPERS=OFF",
        "-DCMAKE_DISABLE_PRECOMPILE_HEADERS=ON"
      ]
    },
    "opencv": {
      "url": "https://github.com/opencv/opencv.git",
      "commit": "95be2f2af8d70c1fa703192d9d338c53323b24a1",
      "cmake_flags": [
        "-DBUILD_TESTS=OFF",
        "-DBUILD_PERF_TESTS=OFF",
        "-DBUILD_EXAMPLES=OFF",
        "-DBUILD_DOCS=OFF",
        "-DBUILD_JAVA=OFF",
        "-DBUILD_opencv_python3=OFF",
        "-DBUILD_opencv_python2=OFF",
        "-DWITH_CUDA=OFF",
        "-DWITH_FFMPEG=OFF",
        "-DWITH_GTK=OFF",
        "-DWITH_VTK=OFF",
        "-DWITH_GSTREAMER=OFF",
        "-DCMAKE_DISABLE_PRECOMPILE_HEADERS=ON"
      ]
    },
    "SFML": {
      "url": "https://github.com/SFML/SFML.git",
      "commit": "51196acddb879ee1a276ba1a39c1fec590114abf",
      "cmake_flags": [
        "-DSFML_BUILD_EXAMPLES=OFF",
        "-DSFML_BUILD_DOC=OFF",
        "-DSFML_BUILD_TEST_SUITE=OFF",
        "-DCMAKE_DISABLE_PRECOMPILE_HEADERS=ON"
      ]
    }
  }
}
